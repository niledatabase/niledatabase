{
  "name": "node_react",
  "version": "1.0.0",
  "description": "Multi-tenant todo list app, built with Nile, NodeJS and React",
  "type": "module",
  "scripts": {
    "dev:fe": "vite",
    "dev:be": "bash bin/dev_be.sh",
    "dev": "concurrently \"npm:dev:fe\" \"npm:dev:be\"",
    "start": "node dist/be/app.js",
    "build:fe": "vite build",
    "build:backend": "tsc --project src/be/tsconfig.be.json",
    "build": "npm run build:backend && npm run build:fe",
    "preview": "vite preview"
  },
  "private": true,
  "proxy": "http://localhost:3000",
  "dependencies": {
    "@fontsource/inter": "^5.0.13",
    "@niledatabase/react": "^4.2.0",
    "@niledatabase/server": "^4.2.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@tailwindcss/postcss": "^4.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "lucide-react": "^0.474.0",
    "openai": "^4.53.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.15.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "packageManager": "npm@10.8.3",
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.6",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^4.1.0",
    "postcss": "^8.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
  },
  "overrides": {
    "@typescript-eslint/typescript-estree": "^5.57.1",
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
