{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:docSearch && npm run build:blogSearch && npm run build:templates && npm run build:sidenav && next build",
    "start": "next start",
    "lint": "next lint",
    "build:templates": "node ./app/templates/_build/templates.mjs",
    "build:sidenav": "node ./app/docs/_build/sidenav.mjs",
    "build:blogSearch": "node -r dotenv/config --experimental-loader=./scripts/blog/loader.mjs ./scripts/blog/generateSearch.mjs",
    "build:docSearch": "node -r dotenv/config --experimental-loader=./scripts/blog/loader.mjs ./scripts/docs/generateSearch.mjs"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.6",
    "algoliasearch": "^4.19.1",
    "lodash": "^4.17.21",
    "next": "^13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.2",
    "react-instantsearch": "^7.0.2",
    "react-remark": "^2.1.0",
    "rehype-highlight": "^6.0.0",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.1.0"
  },
  "devDependencies": {
    "@mdx-js/node-loader": "^2.3.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.81",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.197",
    "@types/mdx": "^2.0.5",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.3.1",
    "glob": "10.3.0",
    "imagemin": "^8.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "postcss": "8.4.24",
    "sharp": "^0.32.5",
    "stream-json": "^1.8.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "yargs": "^17.7.2"
  }
}
