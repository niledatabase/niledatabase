{
  "name": "niledatabase",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "www", 
    "examples/quickstart/nextjs", 
    "examples/user_management/email_login/NextJS", 
    "examples/user_management/sso_login_okta/NextJS",
    "examples/user_management/social_login_google/NextJS",
    "examples/user_management/NextAuth",
    "examples/quickstart/java/fe",
    "examples/quickstart/drizzle/fe",
    "examples/serverless/lambda-drizzle/fe"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:www": "turbo run dev --filter=www",
    "format": "prettier --write \"www/**/*.{js,jsx,ts,tsx,css,md,mdx,json,mjs}\"",
    "build:www": "turbo run build --filter=www"
  },
  "description": "tenant aware, serverless, globally distributed postgres",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheNileDev/niledatabase.git"
  },
  "keywords": [
    "postgres",
    "serverless",
    "user",
    "management",
    "tenant",
    "aware"
  ],
  "author": "jrea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheNileDev/niledatabase/issues"
  },
  "homepage": "https://github.com/TheNileDev/niledatabase#readme",
  "devDependencies": {
    "prettier": "^2.8.8",
    "turbo": "^1.10.13"
  }
}
