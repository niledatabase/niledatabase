{
  "name": "niledatabase",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "www",
    "examples/quickstart/nextjs",
    "examples/user_management/email_login/NextJS",
    "examples/user_management/sso_login_okta/NextJS",
    "examples/user_management/social_login_google/NextJS",
    "examples/user_management/NextAuth",
    "examples/quickstart/java/fe",
    "examples/quickstart/drizzle/fe",
    "examples/quickstart/prisma/fe",
    "examples/serverless/lambda-drizzle/fe",
    "examples/integrations/stripe_subscription",
    "examples/ai/ai_pdf",
    "examples/ai/code_assist"
  ],
  "scripts": {
    "dev": "turbo run dev --concurrency=12",
    "dev:www": "turbo run dev --filter=www",
    "format": "prettier --write \"(www|examples)/**/*.{js,jsx,ts,tsx,css,md,mdx,json,mjs}\"",
    "build:www": "turbo run build --filter=www"
  },
  "description": "tenant aware, serverless, globally distributed postgres",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheNileDev/niledatabase.git"
  },
  "keywords": [
    "postgres",
    "serverless",
    "user",
    "management",
    "tenant",
    "aware"
  ],
  "author": "jrea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheNileDev/niledatabase/issues"
  },
  "homepage": "https://github.com/TheNileDev/niledatabase#readme",
  "devDependencies": {
    "eslint": "9.2.0",
    "prettier": "^2.8.8",
    "turbo": "^2.6.2"
  },
  "overrides": {
    "eslint": "9.2.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-config-next": "^15.3.3",
    "tar-fs": "2.1.3",
    "prismjs": "1.30.0",
    "@radix-ui/react-use-controllable-state": "1.1.1"
  },
  "optionalDependencies": {
    "@img/sharp-linux-x64": "^0.34.2",
    "@swc/core-linux-x64-gnu": "^1.2.160",
    "piscina": "^4.9.2"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
